{
	"manifest_version": 2,
	"name": "Clear Cache",
	"version": "1.0.0",
	"description": "Clears your cache and browsing data with a click of a button",
	"minimum_chrome_version":"19",
	"icons": {
		"16":"img/icon_16.png",
		"48":"img/icon_48.png",
		"128":"img/icon_128.png"
	},
	"background": {
		"page": "backgroundpage.html"
	},
	"browser_action": {
		"default_icon":	"img/icon_19.png",
		"default_title":"Clear Cache"
		
	},
	"options_ui": {
		"chrome_style": true,
		"page": "options.html",
		"open_in_tab": true
	  },
	"permissions": ["browsingData","cookies","<all_urls>"]
}